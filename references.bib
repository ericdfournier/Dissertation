journal = {Journal of environmental management},
keywords = {Conservation of Natural Resources,Conservation of Natural Resources: methods,Environment,Environmental Monitoring,Models, Theoretical,Time,Waste Disposal, Fluid,Waste Disposal, Fluid: methods},
