journal = {Journal of environmental management},
keywords = {Capital Financing,Conservation of Natural Resources,Consumer Participation,Odors,Personnel Staffing and Scheduling,Socioeconomic Factors,Waste Disposal, Fluid,Waste Disposal, Fluid: economics,Waste Disposal, Fluid: standards},
month = aug,
