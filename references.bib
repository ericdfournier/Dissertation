@unpublished{Sheldon2014,
abstract = {Long term water availability is of increasing concern worldwide. In the United States alone, water demand has tripled since the 1950’s, while efficiency measures have only amounted to a savings of roughly one percent. The rapid increase in worldwide brackish and ocean water desalination capacity, in addition to the global proliferation of long haul fresh water transfers is indicative of the lengths to which populations will go to secure their water supplies. Such projects have high capital and operational costs. For example, desalination requires at least ten times more energy to deliver each gallon of fresh water than nearby fresh surface and groundwater sources require. Unfortunately, local fresh water sources can be of limited availability. Over-use of local fresh water supplies can drive ecosystems and ecosystem services to collapse, while over-extraction of groundwater can lead to irreversible salinization. We present a global freshwater allocation model that is optimized for primary energy efficiency. Freshwater sources and potential sources as well as local electricity fuel mixes and other geographic data are used to approximate the amount of energy needed to durably supply water to various locations, such that long term fresh water availability is not put at risk. Results are presented at a 0.1 degree spatial resolution. Initial results indicate that many heavily populated urban areas are overusing local water supplies and underinvesting in the infrastructure needed to avoid long term water availability risk. Furthermore, antagonistic moral and economic signals are creating contradictory signals to water consumers—to the detriment of real water sustainability. The results of this analysis will be used elsewhere to support a partial equilibrium model that estimates the most economically efficient price for residential water for multiple cities worldwide.},
address = {Boston, MA},
author = {Sheldon, Seth D. and Zik, Ory},
file = {:Users/ericfournier/Google Drive/PhD/Papers/Civil and Environmental Engineering/Sheldon\_Zik\_2014.pdf:pdf},
institution = {Energy Points Inc.},
number = {617},
pages = {1--19},
title = {{The Right (Energetic) Price of Water}},
year = {2014}
}
@article{Sheng2005367,
abstract = {The traditional concept of Aquifer Storage and Recovery (ASR) has been emphasized and extensively applied for water resources conservation in arid and semi-arid regions using groundwater systems as introduced in Pyne's book titled Groundwater Recharge and Wells. This paper extends the \{ASR\} concept to an integrated level in which either treated or untreated surface water or reclaimed wastewater is stored in a suitable aquifer through a system of spreading basins, infiltration galleries and recharge wells; and part or all of the stored water is recovered through production wells, dual function recharge wells, or by streams receiving increased discharge from the surrounding recharged aquifer as needed. In this paper, the author uses the El Paso Water Utilities (EPWU) \{ASR\} system for injection of reclaimed wastewater into the Hueco Bolson aquifer as an example to address challenges and resolutions faced during the design and operation of an \{ASR\} system under a new \{ASR\} system definition. This new \{ASR\} system concept consists of four subsystems: source water, storage space-aquifer, recharge facilities and recovery facilities. Even though facing challenges, this system has successfully recharged approximately 74.7 million cubic meters (19.7 billion gallons) of reclaimed wastewater into the Hueco Bolson aquifer through 10 recharge wells in the last 18 years. This \{ASR\} system has served dual purposes: reuse of reclaimed wastewater to preserve native groundwater, and restoration of groundwater by artificial recharge of reclaimed wastewater into the Hueco Bolson aquifer.},
annote = {Substainable planning in a semi-arid fast growing region},
author = {Sheng, Zhuping},
doi = {http://dx.doi.org/10.1016/j.jenvman.2004.10.007},
file = {:Users/ericfournier/Google Drive/PhD/Publications/2014/Environmental Modeling and Software/Papers/Sheng\_2004.pdf:pdf},
issn = {0301-4797},
journal = {Journal of Environmental Management},
keywords = {Aquifer storage and recovery (ASR) system,Groundwater,Reclaimed wastewater,Water quality compatibility},
number = {4},
pages = {367--377},
title = {{An aquifer storage and recovery system with reclaimed wastewater to preserve native groundwater resources in El Paso, Texas}},
url = {http://www.sciencedirect.com/science/article/pii/S0301479705000216},
volume = {75},
year = {2005}
}
