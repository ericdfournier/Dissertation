pages = {1--16},
title = {{Models for the optimization of regional wastewater treatment systems}},
volume = {73},
year = {1994}
}
@article{Menegaki2007,
abstract = {This paper investigates the Willingness to Use (WTU) and Willingness to Pay (WTP) for recycled water in agriculture. We report results from surveys of farmers and consumers on the island of Crete, Greece. Crete is suffering from an increasingly severe water shortage coupled with declining groundwater supplies, therefore the wider use of recycled water is an important policy priority. We have investigated WTU and WTP for two crops with two different levels of water treatment. The mean WTP for 1 cm3 of recycled water was 0.15€ for the irrigation of both olive trees and tomato crops, namely 55\% of the fresh water price. The mean WTP for olive oil produced from olive trees irrigated with recycled water was 2.65€, namely 88\% of its current market price.Wehave found that both attitudinal factors, such as environmental awareness and economic factors, such as freshwater prices and incomes, are significant in explaining theWTUand WTP for recycled water and products produced using it, but that important differences exist between farmers and consumers.},
author = {Menegaki, Angeliki N. and Hanley, Nick and Tsagarakis, Konstantinos P.},
doi = {10.1016/j.ecolecon.2007.01.008},
file = {:Users/ericfournier/Library/Application Support/Mendeley Desktop/Downloaded/Menegaki, Hanley, Tsagarakis - 2007 - The social acceptability and valuation of recycled water in Crete A study of consumers' and farmers' attitudes.pdf:pdf},
issn = {09218009},
journal = {Ecological Economics},
month = apr,
number = {1},
pages = {7--18},
title = {{The social acceptability and valuation of recycled water in Crete: A study of consumers' and farmers' attitudes}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0921800907000924},
volume = {62},
year = {2007}
}
@article{Meneses2010,
abstract = {The main function of a Wastewater Treatment Plant is to minimize the environmental impact of discharging untreated water into natural water systems. Also a Wastewater Treatment Plant may get a resource from wastewater carrying out a tertiary treatment on the treated wastewater which can be reused in non-potable applications. Water reuse strategies are intended to address the problem of water scarcity without aggravating other environmental problems, thus reflecting the need of their environmental assessment. In this paper we used Life Cycle Assessment to evaluate different disinfection treatments (chlorination plus ultraviolet treatment, ozonation and ozonation plus hydrogen peroxide) and to assess the environmental advantages and drawbacks of urban wastewater reuse in non-potable applications. To do so, we compared the environmental impacts of producing 1m(3) of water for non-potable uses from reclaimed water, potable water and desalinated water sources. The calculation has used current operating data from a Wastewater Treatment Plant located in the Mediterranean area, although the results can be applied to any other plant with similar technology. The ozonation and ozonation plus hydrogen peroxide disinfection treatment technologies have similar environmental profiles. However most of the indicators are about 50\% higher than the ultraviolet disinfection except for the acidification (100\% higher) and photochemical oxidation (less than 5\%). Non-potable uses (both agricultural and urban uses) of reclaimed water have environmental and economical advantages. Reuse of treated wastewater is particularly beneficial when it can replace desalinated water. Consequently, reclaimed water should be promoted for non-potable uses, when there is scarcity of freshwater.},
author = {Meneses, Montse and Pasqualino, Jorgelina C and Castells, Francesc},
doi = {10.1016/j.chemosphere.2010.05.053},
file = {:Users/ericfournier/Library/Application Support/Mendeley Desktop/Downloaded/Meneses, Pasqualino, Castells - 2010 - Environmental assessment of urban wastewater reuse treatment alternatives and applications.pdf:pdf},
issn = {1879-1298},
journal = {Chemosphere},
keywords = {Cities,Conservation of Natural Resources,Conservation of Natural Resources: methods,Disinfection,Disinfection: methods,Nitrogen,Nitrogen: analysis,Waste Disposal, Fluid,Waste Disposal, Fluid: methods,Water Pollutants,Water Pollutants: analysis,Water Purification,Water Purification: methods},
month = sep,
number = {2},
pages = {266--72},
pmid = {20580058},
publisher = {Elsevier Ltd},
title = {{Environmental assessment of urban wastewater reuse: treatment alternatives and applications.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20580058},
volume = {81},
year = {2010}
}
