keywords = {environmental impact,recycling and reuse of,urban water systems,water,water management},
number = {1},
pages = {73--84},
title = {{Environmental life cycle assessments for water treatment processes – A South African case study of an urban water cycle}},
url = {http://www.scielo.org.za/scielo.php?script=sci\_arttext\&pid=S1816-79502009000100009\&ing=en},
volume = {35},
year = {2009}
}
