keywords = {Arizona,Conservation of Energy Resources,Conservation of Energy Resources: economics,Costs and Cost Analysis,Decision Making,Electricity,Environmental Policy,Models,Renewable Energy,Renewable Energy: economics,Theoretical,Uncertainty,Water Cycle,Water Supply,Water Supply: economics},
month = feb,
number = {4},
pages = {2139--49},
pmid = {24460528},
title = {{The conservation nexus: valuing interdependent water and energy savings in Arizona.}},
url = {http://pubs.acs.org/doi/abs/10.1021/es4033343 http://www.ncbi.nlm.nih.gov/pubmed/24460528},
volume = {48},
year = {2014}
}
